</head>                    
<body>

    <header class="site-header">
        <div class="site-header-content"> {# Inner wrapper for alignment #}
            <div class="site-brand">
                <a href="{{ '/' | url }}">seeudev</a> 
            </div>
                    {# --- Hamburger Button (Mobile Only) --- #}
                <button class="menu-toggle" id="menu-toggle" aria-label="Toggle Menu" aria-expanded="false" aria-controls="main-nav">
                <svg viewBox="0 0 24 24" width="24" height="24" fill="currentColor" aria-hidden="true">
                    <path d="M3 18h18v-2H3v2zm0-5h18v-2H3v2zm0-7v2h18V6H3z"></path>
                </svg>
                </button>
                    {# --- End Hamburger Button --- #}
            <div class="nav-wrapper" id="nav-wrapper">
                <nav id="main-nav">
                    <a href="{{ '/blog/' | url }}" {% if page.url == ('/blog/' | url) %}class="active"{% endif %}>
                        <span class="nav-number">01.</span> 
                        <span class="nav-text">Blog</span>
                    </a>
                    <a href="{{ '/portfolio/' | url }}" {% if page.url == ('/portfolio/' | url) %}class="active"{% endif %}>
                        <span class="nav-number">02.</span> 
                        <span class="nav-text">Portfolio</span>
                    </a>
                    <a href="{{ '/about/' | url }}" {% if page.url == ('/about/' | url) %}class="active"{% endif %}>
                        <span class="nav-number">03.</span> 
                        <span class="nav-text">About</span>
                    </a>
                    <a href="{{ '/contact/' | url }}" {% if page.url == ('/contact/' | url) %}class="active"{% endif %}>
                        <span class="nav-number">04.</span> 
                        <span class="nav-text">Contact</span>
                    </a>
                </nav>
            </div>
        </div> {# End site-header-content #}
    </header>

    <script>
    const menuToggle = document.getElementById('menu-toggle');
    const navWrapper = document.getElementById('nav-wrapper');
    const body = document.body;

    if (menuToggle && navWrapper) {
        menuToggle.addEventListener('click', () => {
            const isExpanded = menuToggle.getAttribute('aria-expanded') === 'true';
            menuToggle.setAttribute('aria-expanded', !isExpanded);
            navWrapper.classList.toggle('is-open');
            body.classList.toggle('nav-open'); // Optional: Prevent scrolling when menu is open
        });
    }
    </script>

    
